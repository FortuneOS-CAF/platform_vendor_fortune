//
// Prebuilt apps for ReloadedOS
//

// Wallique
android_app_import {
    name: "Wallique",
    apk: "Wallique.apk",
    presigned: true,
    dex_preopt: {
        enabled: false,
    },
}

// GrapheneOS camera
android_app_import {
    name: "Camera",
    apk: "Camera.apk",
    presigned: true,
    product_specific: true,
    overrides: ["Camera2"],
    optional_uses_libs: ["androidx.camera.extensions.impl"]
}
